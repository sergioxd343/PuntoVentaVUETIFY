<CatalogoProveedores>

	<buscar_cve_docente>
		<query>
			SELECT cve_persona, nombre, apellido_peterno, apellido_materno,sexo FROM persona;
		</query>
	</buscar_cve_docente>

	
    <areasCapacitacion>
		<query>
			SELECT cve_area, nombre_area FROM area;
		</query>
	</areasCapacitacion>

	<escalas>
		<query>
			SELECT idEscala, numero FROM escala ORDER BY numero DESC;
		</query>
	</escalas>

</CatalogoProveedores>