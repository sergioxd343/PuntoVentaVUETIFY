<CatalogoSolicitudCapacitacionInterna>

    <buscar_cve_docente>
		<query>
			SELECT * FROM v_empleado;
		</query>
	</buscar_cve_docente>

    <cursos>
		<query>
			SELECT * FROM curso
		</query>
	</cursos>

    <tipoInstructor>
		<query>
			SELECT cve_tipo_instructor, nombre_tipo_instructor FROM tipo_instructor
		</query>
	</tipoInstructor>



    <tablaSolicitud>
        <query>
            select * from solicitud_cursos
        </query>
    </tablaSolicitud>

    <insertarSolicitud>
        <query>
            EXEC insertarSolicitudRegistroCapacitacionInterna 
	            @idAreaF = ?,
	            @idAreaS = ?,
                @nombreCurso = ?,
                @fechaCurso = ?,
                @horaCurso = ?,
                @lugar = ?,
                @nombreFacilitador = ?,
                @programaEducativoS = ?,
	            @programaEducativoF = ?,
                @nombreSolicitante = ?,
                @tipoCurso = ?,
                @puesto = ?,
                @tipoCompetencias = ?,
                @totalDias = ?,
                @totalHoras = ?,
                @numeroParticipantes = ?,
                @objetivo = ?,
                @alcance = ?,
                @metodologia = ?,
                @programaDelCurso = ?,
                @horasPorTema = ?,
                @resultadoDeAprendizaje = ?,
                @perfilDeParticipante = ?
        </query>
    </insertarSolicitud>
    
</CatalogoSolicitudCapacitacionInterna>